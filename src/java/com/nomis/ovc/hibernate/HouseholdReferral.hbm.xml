<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.nomis.ovc.business.HouseholdReferral" table="APP.HOUSEHOLDREFERRAL">
      <id name="id" column="ID" type="int">
        <generator class="identity"/>
    </id>
    <property column="BENEFICIARYID" name="beneficiaryId" type="string"/>
    <property column="BENEFICIARYTYPE" name="beneficiaryType" type="int"/>
    <property column="AGEATREFERRAL" name="ageAtReferral" type="int"/>
    <property column="AGEUNITATREFERRAL" name="ageUnitAtReferral" type="int"/>
    <property column="DATECREATED" name="dateCreated" type="date"/>
    <property column="LASTMODIFIEDDATE" name="lastModifiedDate" type="date"/>
    <property column="DATEOFREFERRAL" name="dateOfReferral" type="date"/>
    <property column="HEALTHSERVICES" name="healthServices" type="string"/>
    <property column="SAFETYSERVICES" name="safetyServices" type="string"/>
    <property column="SCHOOLEDSERVICES" name="schooledServices" type="string"/>
    <property column="STABLESERVICES" name="stableServices" type="string"/>
    <property column="GBVSERVICES" name="gbvServices" type="string"/>
    <property column="NUMBEROFSERVICES" name="numberOfServices" type="int"/>
    <property column="RECORDEDBY" name="recordedBy" type="string"/>
    <property column="COMMUNITYWORKER" name="communityWorker" type="string"/>
    <property column="MARKEDFORDELETE" name="markedForDelete" type="int"/>
    <property column="RECEIVINGORGANIZATION" name="receivingOrganization" type="string"/>
    <property column="REFERRINGORGANIZATION" name="referringOrganization" type="string"/>
    <property column="REFERRALCOMPLETED" name="referralCompleted" type="int"/>
  </class>
</hibernate-mapping>
