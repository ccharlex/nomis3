<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.nomis.ovc.business.Occupation" table="APP.OCCUPATION">
      <id name="recordId" column="RECORDID" type="int">
        <generator class="identity"/>
    </id>
    <property name="name" column="OCCUPATIONNAME" type="string"/>
    <property name="value" column="OCCUPATIONVALUE" type="int"/>
  </class>
</hibernate-mapping>
