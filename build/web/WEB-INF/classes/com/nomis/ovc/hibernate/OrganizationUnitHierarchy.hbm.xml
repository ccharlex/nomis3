<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.nomis.ovc.metadata.OrganizationUnitHierarchy" table="APP.ORGANIZATIONUNITHIERARCHY">
      <id name="uid" column="UID"/>
      <property name="dateCreated" column="DATECREATED" type="date"/>
      <property name="description" column="DESCRIPTION" type="string"/>
      <property name="lastModifiedDate" column="LASTMODIFIEDDATE" type="date"/>
      <property name="name" column="HIERARCHYNAME" type="string"/>
      <property name="oulevel" column="OULEVEL" type="int"/>
      <property name="recordedBy" column="RECORDED" type="string"/>
  </class>
</hibernate-mapping>
