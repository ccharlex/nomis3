<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.nomis.ovc.business.NutritionStatus" table="APP.NUTRITIONSTATUS">
    <id name="ovcId" column="OVCID" type="string"/>
    <property name="currentNutritionalStatus" column="CURRENTNUTRITIONSTATUS" type="int"/>
    <property name="dateOfCurrentNutritionalStatus" column="DATEOFCURRENTNUTRITIONSTATUS" type="date"/>
    <property name="lastModifiedDate" column="LASTMODIFIEDDATE" type="date"/>
    <property name="markedForDelete" column="MARKEDFORDELETE" type="int"/>
  </class>
</hibernate-mapping>
