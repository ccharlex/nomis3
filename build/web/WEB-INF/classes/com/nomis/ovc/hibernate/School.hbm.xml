<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.nomis.ovc.business.School" table="APP.SCHOOL">
      <id name="id" column="ID" type="string"/>
    <property name="schoolName" column="SCHOOLNAME" type="string"/>
    <property name="schoolType" column="SCHOOLTYPE" type="int"/>
    <property name="dateCreated" column="DATECREATED" type="date"/>
    <property name="lastModifiedDate" column="LASTMODIFIEDDATE" type="date"/>
    <property name="markForDelete" column="MARKEDFORDELETE" type="int"/>
    <property name="orgUnitId" column="ORGANIZATIONUNITID" type="string"/>
    <property name="recordedBy" column="RECORDEDBY" type="string"/>
  </class>
</hibernate-mapping>
