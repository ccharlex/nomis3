    <property name="dateOfNextAppointment" column="DATEOFNEXTAPPOINTMENT" type="date"/>
    <property name="dateOfViralLoadSampleCollection" column="DATEOFVIRALLOADTEST" type="date"/>
    
    <property name="dateOfLastDrugPickup" column="DATEOFLASTDRUGPICKUP" type="date"/>
