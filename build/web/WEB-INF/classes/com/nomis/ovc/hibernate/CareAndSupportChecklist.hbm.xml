    <property name="receivedTransportationSupport" column="RECEIVEDTRANSPORTATIONSUPPORT" type="int"/>
    <property name="recordedBy" column="RECORDEDBY" type="string"/>
    <property name="soresRashPainExperience" column="SORESRASHPAINEXPERIENCE" type="int"/>
    <property name="viralLoadResult" column="VIRALLOADRESULT" type="int"/>
    <property name="viralLoadResultKnown" column="VIRALLOADRESULTKNOWN" type="int"/>
    <property name="viralLoadTestDone" column="VIRALLOADTESTDONE" type="int"/>
